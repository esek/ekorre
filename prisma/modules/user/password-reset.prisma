
model PrismaPasswordReset {
	token String @id
	User PrismaUser @relation(fields: [refUser], references: [username])
	refUser String @map("ref_user")
	time DateTime @default(now())

	@@map("password_resets")
}

model PrismaUser {
	username String @id

	passwordResets PrismaPasswordReset[]

	@@map("users")
}