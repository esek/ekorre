model PrismaEmergencyContact {
	id Int @id @default(autoincrement())

	name String
	type String
	phone String

	user PrismaUser @relation(fields: [refUser], references: [username])
	refUser String @map("ref_user")

	@@map("emergency_contacts")
}

model PrismaUser {
	username String @id

	emergencyContacts PrismaEmergencyContact[]

	@@map("users")
}