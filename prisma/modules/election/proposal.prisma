model PrismaProposal {
	id Int @id @default(autoincrement())

	user PrismaUser @relation(fields: [refUser], references: [username])
	refUser String @map("ref_user")

	election PrismaElection @relation(fields: [refElection], references: [id])
	refElection Int @map("ref_election")

	@@map("proposals")
}

model PrismaUser {
	username String @id

	proposals PrismaProposal[]

	@@map("users")
}

model PrismaElection {
	id Int @id @default(autoincrement())

	proposals PrismaProposal[]

	@@map("elections")
}