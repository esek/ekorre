{
  "name": "ekorre-ts",
  "version": "0.1.0",
  "description": "E-Sektionens backend",
  "main": "src/app.ts",
  "scripts": {
    "run": "node build/app.js",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "npm run run",
    "dev": "nodemon -e ts,graphql --watch src --exec \"node --nolazy --inspect -r ts-node/register src/app.ts\"",
    "generate": "graphql-codegen --config codegen.yml",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" \"test/**/*.ts\" --write",
    "lint": "eslint src",
    "test": "jest",
    "unittest": "jest test/unit",
    "regressiontest": "jest test/regression",
    "integrationtest": "jest test/integration"
  },
