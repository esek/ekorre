{
  "name": "ekorre-ts",
  "version": "0.0.1",
  "description": "E-Sektionens backend",
  "main": "src/app.ts",
  "scripts": {
    "run": "node build/app.js",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "npm run run",
    "dev": "nodemon -e ts,graphql --watch src --exec 'node --nolazy --inspect -r ts-node/register src/app.ts'",
    "generate": "graphql-codegen --config codegen.yml",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint src; exit 0"
  },
  "author": "Emil Blennow",
  "bugs": {
    "email": "macapar@esek.se"
  },
  "license": "ISC",
  "dependencies": {
    "@types/showdown": "^1.9.3",
    "apollo-server": "^2.18.2",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.4.1",
    "graphql-tools": "^6.2.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "mariadb": "^2.5.1",
    "regenerator-runtime": "^0.13.7",
    "showdown": "^1.9.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "type-graphql": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@trivago/prettier-plugin-sort-imports": "^1.2.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "sqlite3": "^5.0.0"
  }
}
