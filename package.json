{
  "name": "ekorre-ts",
  "version": "1.2.0",
  "description": "E-Sektionens backend",
  "main": "src/index.ts",
  "scripts": {
    "run": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/src/index.js",
    "build": "tsc --project ./tsconfig.build.json",
    "prestart": "npm run build",
    "start": "npm run run",
    "dev": "nodemon -e ts,graphql --watch src --exec \"node --nolazy --inspect -r tsconfig-paths/register -r ts-node/register src/index.ts\"",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" \"test/**/*.ts\" --write",
    "lint": "eslint src test",
    "test": "jest",
    "test:prebuild": "jest test/unit test/integration",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:regression": "jest test/regression --coverage=false",
    "benchmark": "ts-node tools/benchmark/index.ts",
    "prisma:ci": "prisma generate && prisma db push --accept-data-loss && prisma db seed",
    "prisma:reset": "prisma migrate reset; prisma db push; prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "generate": "npm run graphql:generate && npm run prisma:generate"
  },
  "author": "Emil Blennow <emil.blennow99@gmail.com",
  "contributors": [
    "Emil Eriksson <eje1999@gmail.com>",
    "Axel Froborg <axel.froborg@gmail.com>",
    "Marcus Lindell <lindellmarcus99@gmail.com>"
  ],
  "bugs": {
    "email": "macapar@esek.se"
  },
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/esek/ekorre.git"
  },
  "prisma": {
    "seed": "ts-node prisma/data/seed.ts"
  },
  "dependencies": {
    "@esek/auth-server": "3.0.0",
    "@graphql-tools/graphql-file-loader": "^7.5.2",
    "@graphql-tools/load": "^7.7.4",
    "@graphql-tools/schema": "^9.0.1",
    "@prisma/client": "^4.2.1",
    "apollo-server-core": "^3.10.1",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.10.1",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "jsdom": "^20.0.0",
    "jsonwebtoken": "^8.5.1",
    "regenerator-runtime": "^0.13.9",
    "set-cookie-parser": "^2.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/inquirer": "^9.0.1",
    "@types/jest": "^28.1.7",
    "@types/jsdom": "^20.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.7.6",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "inquirer": "^9.1.0",
    "jest": "^28.1.3",
    "nanospinner": "^1.1.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "prisma-dbml-generator": "^0.9.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8"
  }
}
