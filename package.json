{
  "name": "ekorre-ts",
  "version": "0.10.3",
  "description": "E-Sektionens backend",
  "main": "src/index.ts",
  "scripts": {
    "run": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/src/index.js",
    "build": "tsc --project ./tsconfig.build.json",
    "prestart": "npm run build",
    "start": "npm run run",
    "dev": "nodemon -e ts,graphql --watch src --exec \"node --nolazy --inspect -r tsconfig-paths/register -r ts-node/register src/index.ts\"",
    "generate": "graphql-codegen --config codegen.yml",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" \"test/**/*.ts\" --write",
    "lint": "eslint src",
    "lint:test": "eslint test",
    "test": "jest",
    "test:prebuild": "jest test/unit test/integration",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:regression": "jest test/regression --coverage=false",
    "benchmark": "ts-node tools/benchmark/index.ts"
  },
  "author": "Emil Blennow <emil.blennow99@gmail.com",
  "contributors": [
    "Emil Eriksson <eje1999@gmail.com>",
    "Axel Froborg <axel.froborg@gmail.com>",
    "Marcus Lindell <lindellmarcus99@gmail.com>"
  ],
  "bugs": {
    "email": "macapar@esek.se"
  },
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.5.1",
    "@graphql-tools/merge": "^8.2.1",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/utils": "^8.5.3",
    "apollo-graphql": "^0.9.5",
    "apollo-server": "^3.5.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-errors": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dompurify": "^2.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "graphql": "^15.3.0",
    "graphql-middleware": "^6.1.12",
    "graphql-scalars": "^1.13.6",
    "jsdom": "^16.5.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "mysql2": "^2.2.5",
    "regenerator-runtime": "^0.13.7",
    "set-cookie-parser": "^2.4.8",
    "showdown": "^1.9.1",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.12.0",
    "type-graphql": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@trivago/prettier-plugin-sort-imports": "^1.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dompurify": "^2.2.3",
    "@types/express": "^4.17.8",
    "@types/express-fileupload": "^1.1.6",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/showdown": "^1.9.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.0",
    "inquirer": "^8.2.0",
    "jest": "^27.1.0",
    "nanospinner": "^1.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^6.2.1",
    "ts-jest": "^27.0.5"
  }
}
