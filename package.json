{
  "name": "ekorre-ts",
  "version": "0.14.2",
  "description": "E-Sektionens backend",
  "main": "src/index.ts",
  "scripts": {
    "run": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/src/index.js",
    "build": "tsc --project ./tsconfig.build.json",
    "prestart": "npm run build",
    "start": "npm run run",
    "dev": "nodemon -e ts,graphql --watch src --exec \"node --nolazy --inspect -r tsconfig-paths/register -r ts-node/register src/index.ts\"",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" \"test/**/*.ts\" --write",
    "lint": "eslint src test",
    "lint:ci": "eslint src test --output-file eslint_report.json --format json; exit 0",
    "test": "jest",
    "test:prebuild": "jest test/unit test/integration",
    "test:ci": "npm run test:prebuild; exit 0",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:regression": "jest test/regression --coverage=false",
    "benchmark": "ts-node tools/benchmark/index.ts",
    "prisma:ci": "prisma generate && prisma db push --accept-data-loss && prisma db seed",
    "prisma:reset": "prisma migrate reset && prisma db push && prisma db seed",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "generate": "npm run graphql:generate && npm run prisma:generate"
  },
  "author": "Emil Blennow <emil.blennow99@gmail.com",
  "contributors": [
    "Emil Eriksson <eje1999@gmail.com>",
    "Axel Froborg <axel.froborg@gmail.com>",
    "Marcus Lindell <lindellmarcus99@gmail.com>"
  ],
  "bugs": {
    "email": "macapar@esek.se"
  },
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/data/seed.ts"
  },
  "dependencies": {    
    "@esek/auth-server": "^0.0.7",
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.5.1",
    "@graphql-tools/schema": "^8.3.1",
    "@prisma/client": "^4.0.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-errors": "^3.3.0",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.13.6",
    "jsdom": "^20.0.0",
    "jsonwebtoken": "^8.5.1",
    "regenerator-runtime": "^0.13.7",
    "set-cookie-parser": "^2.4.8",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@trivago/prettier-plugin-sort-imports": "^1.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/express-fileupload": "^1.1.6",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.6",
    "eslint-plugin-import": "^2.22.0",
    "inquirer": "^8.2.0",
    "jest": "^27.1.0",
    "nanospinner": "^1.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "prisma": "^4.0.0",
    "prisma-dbml-generator": "^0.9.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.0.5"
  }
}
